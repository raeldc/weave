{
    "env": {
        "development": {
            "application/javascript": {
                "presets": ["es2016-node5", "react"],
                "sourceMaps": "inline"
            },
            "text/less": {
                "dumpLineNumbers": "comments",
                "paths": [
                    "./public/css/less"
                ]
            }
        },
        "production": {
            "application/javascript": {
                "presets": ["es2016-node5", "react"],
                "sourceMaps": "none"
            }
        }
    }
}